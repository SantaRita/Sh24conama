	<queryString>
		<![CDATA[SELECT DISTINCT 
               web.cdprovee ,
               fra.txfactur AS numfra,
               round(fra.impagdiv,2) total_factura,
               fra.fhvencim,fra.fhfactur,
               fra.rgfactur,
               web_prov.datos_factura(web.cdprovee, web.cdasiste || web.numorden, fra.rgfactur,'NOM') nombre,
               web_prov.datos_factura (web.cdprovee, web.cdasiste || web.numorden, fra.rgfactur,'DOM') domicilio,
               web_prov.datos_factura (web.cdprovee, web.cdasiste || web.numorden, fra.rgfactur,'NIF') nif,
               web_prov.datos_factura (web.cdprovee, web.cdasiste || web.numorden, fra.rgfactur,'POB') poblacion,
               web_prov.datos_factura (web.cdprovee, web.cdasiste || web.numorden, fra.rgfactur,'PROV') provincia ,             
               emi.nombre emisor_nombre,
               emi.nif emisor_nif,
               emi.domicilio emisor_domicilio,
               emi.poblacion emisor_poblacion                

               ,TO_CHAR (fra.rgfactur) rgfactur,
                               'FAB' estadoexp, web.cdasiste cdasiste,
                               web.cdprovee, fra.txfactur AS numfra,
                               TO_CHAR (ROUND (fra.impagdiv, 2),
                                        '999G999G990D00'
                                       ) importe,
                               TO_CHAR (fra.fhfactur,
                                        'DD/MM/YYYY'
                                       ) fecha_factura,
                               NULL numcom, NULL concepto, NULL imbaseimp,
                               NULL imiva, NULL tipo_iva
                               
                          FROM exuah_asignacion_expediente web,
                               ex_movimientos mov,
                               cp_facturas_pago fra,
                               cpuah_emidest_datosimpr emi,
                               CPUAH_FACTURAS_DATOSIMPR DI
                         WHERE web.cdasiste = mov.cdasiste
                           AND web.numorden = mov.nuorden
                           AND web.cdprovee = mov.cdprovee

                           and di.rgfactur = fra.rgfactur
                           and di.id_emisor = emi.rgemidest
							and di.tpfactur = 'F'

                           AND mov.cdestado <> 'A'
                           AND mov.rgfactur = fra.rgfactur
                           AND fra.cdestado IN ('T', 'E', 'R', 'P')
   and FRA.RGFACTUR =  $P{RGFACTUR}]]>
	</queryString>
