	<queryString>
		<![CDATA[SELECT  
  c.total_linea,
  pcimpues,
  c.servicio servicio,
  tipo,
  rgfactur_sh,
  cdservic,
  nunivel,
  cdelem,
  dselem || ' ' || ( select '
Material no recigido en baremo: ' || decode(d.asegperj,'A','(A)ASEGURADO - :','P','(P)PERJUDICADO - : ') || d.otros_trabajos cdelem
                from exuah_detalle_presupuesto d 
                where c.rgmovimi = d.rgmovimi
                and cdelem_baremo = c.cdelem
                and cdelem_baremo like '%99'
                and d.otros_trabajos is not null ) dselem , 
  unidades, 
  impbase, 
  rgmovimi, 
  rgmovimi_sh,
  fhfactur, 
  txfactur, 
  fhvencim, 
  cdasiste, 
  cdimpues,
  aseg_perj, 
  iddetalle,
  iddetalle_r,
  round(impbase * ( pcimpues / 100 ),2) iva_importe,
  round(nvl(porcen_retencion,0) * impbase,2) retencion
FROM ( SELECT DISTINCT
       (  SELECT CDRETENC_SAP
          FROM cpuah_retenciones ret
          WHERE em.rgmovimi = EM.RGMOVIMI
          AND em.cdretenc = ret.cdretenc(+) ) PORCEN_RETENCION,
         ff.impbase / ff.unidades total_linea,
         imp.pcimpues,
         maestro_servicio.dselem servicio,
         ff.tipo,
         ff.rgfactur_sh,
         ff.cdservic,
         ff.nunivel,
         ff.cdelem,
         ff.dselem, 
         ff.unidades, 
         ff.impbase, 
         ff.rgmovimi, 
         ff.rgmovimi_sh,
         fhfactur, 
         txfactur, 
         fhvencim, 
         cdasiste, 
         em.cdimpues,
         DECODE (ff.asegperj, 'A', 'ASEG', 'PERJ') aseg_perj, 
         ff.iddetalle,
         ff.iddetalle_r
         
           FROM cpuah_detalle_factura_sh ff,
                cpuah_detalle_factura_sh maestro_servicio,
                ex_movimientos em,
                cp_facturas_pago fp,
                cpuah_impuestos imp

          WHERE ff.rgfactur_sh = $P{RGFACTUR}
            AND em.rgfactur = ff.rgfactur_sh
            AND fp.rgfactur = em.rgfactur

            AND MAESTRO_SERVICIO.rgfactur_sh = ff.rgfactur_sh 
            AND MAESTRO_SERVICIO.NUNIVEL = 1 
            and ff.nunivel <> 2
            AND MAESTRO_SERVICIO.IDDETALLE = ff.iddetalle_r 
            and em.cdimpues =  imp.cdimpues
) c]]>
	</queryString>
