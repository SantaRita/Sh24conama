	<parameter name="tipo" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT  'FAB' estadoexp,
                web.cdasiste idexpediente,
                fra.txfactur AS numfra,
                TO_CHAR (ROUND (fra.impagdiv, 2),'999G999G990D00') importe,
                DECODE(fra.cdestado,'P', TO_CHAR (fra.fhvencim, 'DD/MM/YYYY'),'''') fecha_vencimiento,
                TO_CHAR (fra.fhfactur,'DD/MM/YYYY') fecha_factura,
                '' concepto, 
                CASE fra.cdestado
                                  WHEN 'P'
                                     THEN NVL
                                            ((SELECT 'PAGADA'
                                                FROM DUAL
                                               WHERE SYSDATE >= fra.fhvencim),
                                             (SELECT 'A VENCIMIENTO'
                                                FROM DUAL
                                               WHERE SYSDATE < fra.fhvencim)
                                            )
                                  ELSE 'EN TRAMITE'
                END estado_fact,
                TO_CHAR (ROUND (fra.imfacctv, 2),'FM999G999G990D00') imbaseimp,
                null iva,
                TO_CHAR (fra.rgfactur) rgfactur                
             ,
                            web_prov.datos_factura(web.cdprovee, web.cdasiste || web.numorden, fra.rgfactur,'NOM') nombre,
               web_prov.datos_factura (web.cdprovee, web.cdasiste || web.numorden, fra.rgfactur,'DOM') domicilio,
               web_prov.datos_factura (web.cdprovee, web.cdasiste || web.numorden, fra.rgfactur,'NIF') nif,
               web_prov.datos_factura (web.cdprovee, web.cdasiste || web.numorden, fra.rgfactur,'POB') poblacion,
               web_prov.datos_factura (web.cdprovee, web.cdasiste || web.numorden, fra.rgfactur,'PROV') provincia ,
              null  factura_comision
                FROM exuah_asignacion_expediente web,
                     ex_movimientos mov,
                     cp_facturas_pago fra
                WHERE $P{tipo} = 'FAB' 
                          AND web.cdasiste = mov.cdasiste
                           AND web.numorden = mov.nuorden
                           AND web.cdprovee = mov.cdprovee
                           AND mov.cdestado <> 'A'
                           AND mov.rgfactur = fra.rgfactur
                           AND fra.cdestado IN ('T', 'E', 'R', 'P')
                           AND web.cdasiste = NVL ($P{expediente}, web.cdasiste)
                           AND web.cdprovee = SUBSTR (UPPER ($P{cdusuario}), 6)
                           AND FRA.txfactur = NVL($P{factura},FRA.txfactur)
                           AND TO_CHAR(fra.fhvencim, 'YYYYMMDD') >= nvl($P{fecfacvtoini},TO_CHAR(fra.fhvencim, 'YYYYMMDD'))
                           AND TO_CHAR(fra.fhvencim, 'YYYYMMDD') <= nvl($P{fecfacvtofin},TO_CHAR(fra.fhvencim, 'YYYYMMDD'))

                           
UNION
SELECT  'FCO' estadoexp,
             web.cdasiste idexpediente,
             NVL(web_prov.obtener_txfactur_comision(fra.rgfactur),fra.txfactur) AS numfra,
             TO_CHAR (ROUND (fra.imfactot, 2),'FM999G999G990D00') importe,
             TO_CHAR (fra.fhvencim,'DD/MM/YYYY') fecha_vencimiento,
             TO_CHAR (fra.fhfactur, 'DD/MM/YYYY') fecha_factura,
             'Comisiones expediente ' || web.cdasiste || web.numorden || ', Factura: '|| fra.txfactur concepto,
             NULL estado_fact,             
             TO_CHAR (ROUND (fra.imbaseimp, 2),'FM999G999G990D00') imbaseimp,
             fra.tipo_iva iva,
             TO_CHAR (fra.rgfactur) rgfactur
             ,
                            web_prov.datos_factura(web.cdprovee, web.cdasiste || web.numorden, fra.rgfactur,'NOM') nombre,
               web_prov.datos_factura (web.cdprovee, web.cdasiste || web.numorden, fra.rgfactur,'DOM') domicilio,
               web_prov.datos_factura (web.cdprovee, web.cdasiste || web.numorden, fra.rgfactur,'NIF') nif,
               web_prov.datos_factura (web.cdprovee, web.cdasiste || web.numorden, fra.rgfactur,'POB') poblacion,
               web_prov.datos_factura (web.cdprovee, web.cdasiste || web.numorden, fra.rgfactur,'PROV') provincia ,
               fra.cdcomision factura_comision
       FROM exuah_asignacion_expediente web,
            cpuah_facturas_comision fra
       WHERE $P{tipo} = 'FCO' 
                          AND fra.estado IN ('P', 'E')
         AND web.cdasiste = fra.cdasiste
         AND web.cdprovee = fra.cdprovee
         AND web.cdasiste =
                           NVL ($P{expediente}, web.cdasiste)
         AND web.cdprovee = SUBSTR (UPPER ($P{cdusuario}), 6)
                           
        AND FRA.txfactur = NVL($P{factura},FRA.txfactur)
                          
        AND TO_CHAR(fra.fhvencim, 'YYYYMMDD') >= nvl($P{fecfacvtoini},TO_CHAR(fra.fhvencim, 'YYYYMMDD'))
        AND TO_CHAR(fra.fhvencim, 'YYYYMMDD') <= nvl($P{fecfacvtofin},TO_CHAR(fra.fhvencim, 'YYYYMMDD'))

]]>
	</queryString>
