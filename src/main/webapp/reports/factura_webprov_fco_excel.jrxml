	<queryString>
		<![CDATA[SELECT 
       WEB.CDPROVEE,
             NVL(web_prov.obtener_txfactur_comision(fra.rgfactur),fra.txfactur)  numfra,
             round(fra.imfactot) total_factura,
             fra.fhvencim,
             fra.fhfactur,
             FRA.rgfactur,
             des.nombre_des nombre,
             des.domicilio_des domicilio,
             des.nif_des nif,
             des.poblacion_des poblacion,
             '' provincia ,
(  SELECT  
DSFORPAG            CLI_DSDORPAG 
FROM PV_PROVEEDORES PRO,GE_DATOS_PERSONA PER, CP_FORMAS_PAGO FP 
WHERE PRO.RGPERSON=PER.RGPERSON
AND FP.CDFORPAG=PER.CDFORPAG
AND PRO.CDPROVEE=web.cdprovee ) FORMA_PAGO,

WEB_PROV.F_DETALLE_FACTURA_COMISION( WEB.CDASISTE, FRA.RGFACTUR ) DETALLE_FACTURA,


         	 'SERVIHOGAR GESTIÓN 24 HORAS, S.L' emisor_nombre,
             'NIF B64216666' emisor_nif,
             'AV. DELS VENTS, 9-13 B' emisor_domicilio,
             '08017 - BADALONA' emisor_poblacion,              
             TO_CHAR (fra.rgfactur) rgfactur,
             'FCO' estadoexp,
             WEB.CDASISTE,
             WEB.CDPROVEE,
             fra.txfactur AS numfratxfactur,
             TO_CHAR (ROUND (fra.imbaseimp, 2),'FM999G999G990D00') IMPORTE,
             TO_CHAR (fra.fhfactur, 'DD/MM/YYYY') fecha_factura,
             NULL numcom, 
             NULL concepto, 
             NULL imbaseimp,
             FRA.imiva,
             FRA.tipo_iva tipo_iva  , 
             FRA.imfactot TOTAL     
                          
             
       FROM exuah_asignacion_expediente web,
            cpuah_facturas_comision fra,
            (  SELECT rgfactur, di.rgfactur AS rgfactur_di, des.nombre AS nombre_des,
               des.nif AS nif_des, des.domicilio AS domicilio_des,
               des.poblacion AS poblacion_des,
               TO_CHAR (des.telefono, '999G999G999') AS telefono_des
               FROM cpuah_facturas_datosimpr di, cpuah_emidest_datosimpr des
               WHERE di.tpfactur = 'C'
               AND des.rgemidest = di.id_destinatario
             ) des
       WHERE 
         fra.estado IN ('P', 'E')
         AND web.cdasiste = fra.cdasiste
         AND web.cdprovee = fra.cdprovee
        AND fra.RGFACTUR = DES.RGFACTUR
            and FRA.RGFACTUR =  $P{RGFACTUR}]]>
	</queryString>
